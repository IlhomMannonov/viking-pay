    <h2>Чек об оплате</h2>

    <div class="info">
        <div class="line">
            <div class="label">ID транзакции</div>
            <div class="value"><%= data.id %></div>
        </div>

        <div class="line">
            <div class="label">Название</div>
            <% if (data.type === 'provider') { %>
                <% if (data.program) { %>
                    <div class="value">Пополнение конторы</div>
                <% }else{ %>
                    <div class="value">Вывод конторы</div>
                <% } %>
            <% }else{ %>
                <% if (data.program) { %>
                    <div class="value">Пополнение кошелек</div>
                <% }else{ %>
                    <div class="value">Вывод кошелек</div>
                <% } %>
            <% } %>
        </div>

        <% if (data.type === 'provider') { %>
            <div class="line">
                <div class="label">Контора</div>
                <div class="value"><%= data.provider.name %></div>
            </div>
        <% } %>
        <div class="line">
            <div class="label">ID пользователя</div>
            <div class="value"><%= data.user.id %></div>
        </div>
        <% if (data.type === 'provider') { %>
            <div class="line">
                <div class="label">ID Операция</div>
                <div class="value"><%= data.operation_id %>></div>
            </div>
        <% } %>

        <div class="line">
            <div class="label">Пользователь</div>
            <div class="value"><%= data.user.first_name + ' ' + data.user.last_name %></div>
        </div>
        <div class="line">
            <div class="label">ID Пользователь</div>
            <div class="value"><%= data.user.id %></div>
        </div>
        <% if (data.type === 'wallet') { %>
            <div class="line">
                <div class="label">Номер карты</div>
                <div class="value"><%= data.card_number %></div>
            </div>
        <% } %>

        <div class="line">
            <div class="label">Дата транзакции</div>
            <div class="value"><%= formatDate(data.created_at) %></div>
        </div>

        <div class="line">
            <div class="label">Статус транзакции</div>
            <div class="value">
                <% if (data.status === 'success_pay') { %>
                    <span class="status success">Успешно</span>
                <% } else if (data.status === 'reject') { %>
                    <span class="status reject">Отказ</span>
                <% } else if (data.status === 'pending') { %>
                    <span class="status pending">Обработка</span>
                <% } else { %>
                    <span class="status unknown">Неизвестно</span>
                <% } %>
            </div>
        </div>

    </div>

    <div class="amount">
        <div class="label">Сумма</div>
        <div class="value big"><%= formatAmount(data.amount) %></div>
    </div>

    <%
    function formatDate(date) {
        const d = new Date(date);
        const dd = String(d.getDate()).padStart(2, '0');
        const mm = String(d.getMonth() + 1).padStart(2, '0');
        const yyyy = d.getFullYear();
        const hours = String(d.getHours()).padStart(2, '0');
        const minutes = String(d.getMinutes()).padStart(2, '0');
        return `${dd}.${mm}.${yyyy}, ${hours}:${minutes}`;
    }
    %>
    <%
    function formatAmount(amount) {
        if (typeof amount !== 'number') amount = parseFloat(amount)
        return amount.toLocaleString('ru-RU') + ' сум';
    }
    %>
</div>
</body>
